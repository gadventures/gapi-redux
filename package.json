{
  "name": "gapi-redux",
  "version": "4.1.1",
  "description": "A redux client for G Adventures' API",
  "main": "build/gapi-redux.js",
  "module": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server -d --inline --history-api-fallback --config webpack.development.js",
    "build": "webpack --display-error-details -p --progress --colors --config webpack.production.js",
    "build:watch": "webpack --display-error-details -p --progress --colors --watch --config webpack.production.js",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Sia Malekpour",
  "license": "MIT",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "gapi-js": "^3.0.0",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.5"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "clean-webpack-plugin": "^0.1.15",
    "html-webpack-plugin": "2.22.0",
    "jest-cli": "^20.0.3",
    "regenerator-runtime": "^0.10.5",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.4",
    "webpack-merge": "0.14.1",
    "webpack-validator": "2.2.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gadventures/gapi-redux.git"
  },
  "homepage": "https://github.com/gadventures/gapi-redux",
  "bugs": {
    "url": "https://github.com/gadventures/gapi-redux/issues"
  },
  "keywords": [
    "gapi",
    "redux"
  ]
}
